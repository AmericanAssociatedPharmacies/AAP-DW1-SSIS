                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT dt.ServerName
, dt.DbName
, CONVERT(VARCHAR(100), OBJECT_NAME(dt.object_id)) AS ObjectName
, CONVERT(VARCHAR(100), OBJECT_SCHEMA_NAME(dt.object_id)) AS SchemaName
, dt.IndexName
, dt.index_id
, dt.IndexSizeKB
, dt2.user_seeks
, dt2.user_scans
, dt2.user_lookups
, dt2.user_updates
, dt2.[last_user_seek]
, dt2.last_user_scan
, dt2.last_user_lookup
, dt2.last_user_update
--INTO ODSIntake.dbo.TableIndexUsage
FROM(SELECT CONVERT(VARCHAR(100),@@SERVERNAME) AS ServerName
	, CONVERT(VARCHAR(100), DB_NAME()) AS DbName
	, i.object_id
	, CONVERT(VARCHAR(100),ISNULL(i.[name], 'HEAP')) AS IndexName
	, s.index_id
		, (SUM(s.[used_page_count]) * 8) AS IndexSizeKB
	FROM sys.dm_db_partition_stats AS s
	INNER JOIN sys.indexes AS i ON s.[object_id] = i.[object_id]
							AND s.[index_id] = i.[index_id]
	GROUP BY i.object_id
	, i.[name]
	, s.index_id
	) AS dt
LEFT OUTER JOIN(SELECT object_id, index_id, user_seeks, user_scans, user_lookups, user_updates, [last_user_seek], last_user_scan, last_user_lookup, last_user_update
				FROM sys.dm_db_index_usage_stats
				WHERE database_id = DB_ID()
					) AS dt2 ON dt.object_id = dt2.object_id
								and dt.index_id = dt2.index_id
WHERE OBJECT_SCHEMA_NAME(dt.object_id) &lt;&gt; 'sys'
ORDER BY dt.object_id ASC
, dt.IndexSizeKB DESC
, dt.index_id ASC</property>
